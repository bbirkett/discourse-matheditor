<DModal @closeModal={{@dismissKeyboardAndClose}} @title={{i18n (theme-prefix 'editor_opened')}} class="matheditor-modal">
  <:body>
    <div id='mathfield' style='border: 1px solid #999;padding:5px;'></div>
  </:body>
  <:footer>
    <DButton
      @action={{this.insertEq}}
      @class="btn-primary"
      @label={{theme-prefix "insert_equation"}}
      @disabled={{this.disableInsert}}
    />
  </:footer>
</DModal>